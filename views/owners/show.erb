<h1><%= @owner.name() %></h1>

<h2>Pets Owned</h2> 
<table>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th>Species</th>
    <th>Breed</th>
  </tr>
<% for animal in @pets_owned %>
  <tr>
    <td><a href="/animals/<%= animal.id() %>"><%= animal.name() %></a></td>
    <td><%= animal.age() %></td>
    <td><%= animal.species() %></td>
    <td><%= animal.breed() %></td>
  </tr>
<% end %>
</table>